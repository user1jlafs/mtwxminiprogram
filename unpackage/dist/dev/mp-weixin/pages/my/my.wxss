@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.container {
  width: 100%;
  height: 100%;
  background-color: #F8F8F8;
  box-sizing: border-box;
  overflow-x: hidden;
  /* 状态栏样式 */
}
.container .status-bar {
  width: 100%;
  background-color: #8B5A2B;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}
.container .container-box {
  width: 100%;
  padding-top: 0;
  margin-top: 0;
}

